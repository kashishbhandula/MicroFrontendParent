{
  "name": "test3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true next dev",
    "build": "npm install && next build",
    "start": "node server.js",
    "lint": "next lint",
    "check-build": "next build",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext js",
    "format": "prettier --ignore-path .prettierignore --write .",
    "lint-fix": "eslint --fix --ext .js,.jsx .",
    "test-all": "npm run check-format && npm run check-lint && npm run build",
    "prepare": "husky install",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "source-map-analyze": "source-map-explorer .next/static/*/.js",
    "build:analyze": "npm run build && npm run source-map-analyze"
  },
  "dependencies": {
    "@module-federation/nextjs-mf": "^8.3.26",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.1"
  }
}
